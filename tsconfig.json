{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowJs": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "noUnusedLocals": true,
    "isolatedModules": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "noUnusedParameters": true,
    "outDir": "./dist/",
    "baseUrl": "./AddressBook/src",
    "project": "./"
  },
  "include": ["./AddressBook/src"],
  "exclude": ["node_modules", "./AddressBook/src/@types"]
}
